{
  "name": "code-readers-stack",
  "displayName": "Code Reader's Stack",
  "description": "Add bookmarks in a stack fashion",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "crs-outline",
          "name": "Code Reader's Stack"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "command": "code-readers-stack.push",
          "group": "navigation"
        },
        {
          "command": "code-readers-stack.pop",
          "group": "navigation"
        }
      ]
    },
    "commands": [{
      "command": "code-readers-stack.push",
        "title": "CRS Push"
      },
      {
        "command": "code-readers-stack.pop",
        "title": "CRS Pop"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.21.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
